{
  "name": "marvelql",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "bin": {
    "ts-node": "dist/bin.js"
  },
  "engines": {
    "node": "8.10.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ts": "ts-node -T -r esm -r dotenv/config",
    "ts:watch": "ts-node-dev --no-notify --transpileOnly -r esm -r dotenv/config",
    "start": "yarn ts packages/graphql/index.ts",
    "dev": "yarn ts:watch packages/graphql/index.ts",
    "deploy": "bash ./.scripts/deploy.sh",
    "rook": "yarn workspace",
    "board": "yarn workspaces",
    "data": "yarn workspace @marvelql/data download"
  },
  "dependencies": {
    "esm": "^3.2.0",
    "wsrun": "^3.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.1",
    "@typescript-eslint/parser": "^1.4.1",
    "apollo": "^2.6.2",
    "babel-plugin-module-resolver": "^3.1.2",
    "dotenv": "^6.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.2.0",
    "eslint-plugin-relay": "^1.0.0",
    "now": "14.2.2",
    "prisma": "^1.26.6",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.2.2"
  }
}
